package com.foxy.data;

import java.io.Serializable;
import java.util.Date;

/**
 *
 * @author eric
 */
public class FoxyOrderList  implements Serializable {
    /**
     * serial id for serialisation versioning
     */
    private static final long serialVersionUID = 1L;
    private int _id;
    private String _type;
    private String _color;    
    
    private String orderId = null;
    private String customerName = null;
    private String countryName = null;
    private Date orderDate = null;

    public FoxyOrderList() {}
    
    public FoxyOrderList(int id, String type, String color)
    {
        _id = id;
        _type = type;
        _color = color;
    }

    public FoxyOrderList(String orderId, String customer, String country, Date orderDate) {
        this.orderId = orderId;
        this.customerName = customer;
        this.countryName = country;
        this.orderDate = orderDate;
    }

    //PROPERTY: orderId
    public String getOrderId(){
        return this.orderId;
    }
    public void setOrderId(String newValue) {
        this.orderId = newValue;
    }
    
    //PROPERTY: countryCode
    public String getCountryName(){
        return this.countryName;
    }
    public void setCountryName(String newValue) {
        this.countryName = newValue;
    }
    
    //PROPERTY: orderDate
    public Date getOrderDate(){
        return this.orderDate;
    }
    public void setOrderDate(Date newValue) {
        this.orderDate = newValue;
    }
    
    //PROPERTY: customerName
    public String getCustomerName(){
        return this.customerName;
    }
    public void setCustomerName(String newValue) {
        this.customerName = newValue;
    }    
    
    public int getId()
    {
        return _id;
    }

    public void setId(int id)
    {
        _id = id;
    }

    public String getType()
    {
        return _type;
    }

    public void setType(String type)
    {
        _type = type;
    }

    public String getColor()
    {
        return _color;
    }

    public void setColor(String color)
    {
        _color = color;
    }
}
