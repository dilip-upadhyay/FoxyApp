<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.foxy.db.OrderNoReplaced" table="orderno_replaced">
        <id column="ordreplace_id" name="ordReplaceId">
            <generator class="native"/>
        </id>
        <property column="oldorderid" name="oldOrderId"/>
        <property column="neworderid" name="newOrderId"/>
        <property column="status" name="status"/>
        <property column="delusrid" name="delUsrId"/>
        <property column="deltime" name="delTime"/>
        <property column="updusrid" name="updUsrId"/>
        <property column="updtime" name="updTime"/>
        <property column="insusrid" name="insUsrId"/>
        <property column="instime" name="insTime"/>
    </class>
</hibernate-mapping>
