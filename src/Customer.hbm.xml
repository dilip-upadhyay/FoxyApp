<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.foxy.db.Customer" table="customer">
        <id name="custCode" column="custcode"/>
        
        <property name="custName" column="custname"/>
        <property name="federated" column="federated"/>
        <property name="payTerm" column="payterm"/>
        <property name="custAddress" column="custaddress"/>
        <property name="custTel" column="custtel"/>
        <property name="custFax" column="custfax"/>
        <property name="remark" column="remark"/>
        <property name="status" column="status"/>
        <property name="delUsrId" column="delusrid"/>
        <property name="delTime" column="deltime"/>
        <property name="updUsrId" column="updusrid"/>
        <property name="updTime" column="updtime"/>
        <property name="insUsrId" column="insusrid"/>
        <property name="insTime" column="instime"/>        
    </class>
</hibernate-mapping>