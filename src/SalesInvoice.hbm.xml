<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.foxy.db.SalesInvoice" table="salesinvoice">
        <id name="saleinvid" column="saleinvid">
            <generator class="increment"/>
        </id>            
        
        <property name="invno" column="invno"/>
        <property name="invdate" column="invdate"/>
        <property name="custcode" column="custcode"/>
        <property name="custbrand" column="custbrand"/>
        <property name="destination" column="destination"/>
        <property name="stylecode" column="stylecode"/>
        <property name="refdoc" column="refdoc"/>        
        <property name="cmtval" column="cmtval"/>
        <property name="fobval" column="fobval"/>
        <property name="cmtbaseval" column="cmtbaseval"/>
        <property name="fobbaseval" column="fobbaseval"/>
        <property name="currency" column="currency"/>
        <property name="forexrate" column="forexrate"/>
        <property name="revenue" column="revenue"/>
        <property name="shipmode" column="shipmode"/>
        <property name="vessel" column="vessel"/>
        <property name="revenuebaseval" column="revenuebaseval"/>        
        <property name="remarks" column="remarks"/>
        <property name="status" column="status"/>
        <property name="delUsrId" column="delusrid"/>
        <property name="delTime" column="deltime"/>
        <property name="updUsrId" column="updusrid"/>
        <property name="updTime" column="updtime"/>
        <property name="insUsrId" column="insusrid"/>
        <property name="insTime" column="instime"/>        
    </class>
</hibernate-mapping>
