<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.foxy.db.Inventory" table="inventory">
        <id name="id" column="invrefid">
            <generator class="increment"/>
        </id>            
        <property name="supplier" column="supplier"/>
        <property name="invoice" column="invoice"/>
        <property name="invoiceDate" column="invdate"/>
        <property name="invType" column="type"/>
        <property name="invCode" column="code"/>
        <property name="quantity" column="quantity"/>
        <property name="unit" column="unit"/>
        <property name="value" column="value"/>
        <property name="currency" column="currency"/>
        <property name="baseValue" column="basevalue"/>
        <property name="unitCost" column="unitcost"/>           
        <property name="itemDesc" column="itemdesc"/>
        <property name="remarks" column="remarks"/>
        <property name="forexrate" column="forexrate"/>
        <property name="status" column="status"/>
        <property name="delUsrId" column="delusrid"/>
        <property name="delTime" column="deltime"/>
        <property name="updUsrId" column="updusrid"/>
        <property name="updTime" column="updtime"/>
        <property name="insUsrId" column="insusrid"/>
        <property name="insTime" column="instime"/>        
    </class>
</hibernate-mapping>