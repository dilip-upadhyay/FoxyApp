<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.foxy.db.OrderConfirm" table="ordconfirm">
        <id name="id" column="crefid">
            <generator class="increment"/>
        </id>
        
        <property name="orderId" column="orderid"/>
        <property name="ssRefId" column="srefid"/>
        <property name="month" column="month"/>
        <property name="location" column="location"/>
        <property name="subLocation" column="sublocation"/>
        <property name="make" column="make"/>
        <property name="poNumber" column="ponumber"/>
        <property name="poDate" column="podate"/>
        <property name="qtyDzn" column="qtydzn"/>
        <property name="qtyPcs" column="qtypcs"/>
        <property name="unitc" column="unitc"/>
        <property name="fabricDate" column="fabricdate"/>
        <property name="vesselDate" column="vesseldate"/>
        <property name="closeDate" column="closedate"/>
        <property name="ship" column="ship"/>
        <property name="emb" column="emb"/>
        <property name="lmerchandiser" column="lmerchandiser"/>
        <property name="mrForCust" column="mrforcust"/>        
        <property name="remark" column="remark"/>
        <property name="status" column="status"/>
        <property name="delUsrId" column="delusrid"/>
        <property name="delTime" column="deltime"/>
        <property name="updUsrId" column="updusrid"/>
        <property name="updTime" column="updtime"/>
        <property name="insUsrId" column="insusrid"/>
        <property name="insTime" column="instime"/>            
    </class>
</hibernate-mapping>