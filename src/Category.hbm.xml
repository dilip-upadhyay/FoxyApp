<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.foxy.db.Category" table="category">
        <id name="catId" column="catid">
            <generator class="native"/>
        </id>
        
        <property name="category" column="category"/>
        <property name="country" column="country"/>
        <property name="fabric" column="fabric"/>
        <property name="desc" column="description"/>
        <property name="unit" column="unit"/>
        <property name="status" column="status"/>
        <property name="delUsrId" column="delusrid"/>
        <property name="delTime" column="deltime"/>        
        <property name="updUsrId" column="updusrid"/>
        <property name="updTime" column="updtime"/>
        <property name="insUsrId" column="insusrid"/>
        <property name="insTime" column="instime"/>
    </class>
</hibernate-mapping>