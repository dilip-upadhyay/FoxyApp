<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.foxy.db.InvMovement" table="invmovement">
        <id name="id" column="invmrefid">
            <generator class="increment"/>
        </id>            
        <property name="invRefId" column="invrefid"/>
        <property name="ssRefId" column="srefid"/>
        <property name="ttDate" column="tdate"/>
        <property name="quantity" column="quantity"/>
        <property name="usedQty" column="usedqty"/>
        <property name="unitCost" column="unitcost"/>
        <property name="focVal" column="focval"/>
        <property name="itemDesc" column="itemdesc"/>
        <property name="status" column="status"/>
        <property name="delUsrId" column="delusrid"/>
        <property name="delTime" column="deltime"/>
        <property name="updUsrId" column="updusrid"/>
        <property name="updTime" column="updtime"/>
        <property name="insUsrId" column="insusrid"/>
        <property name="insTime" column="instime"/>        
    </class>
</hibernate-mapping>