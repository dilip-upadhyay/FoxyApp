<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.foxy.db.SalesInvoiceDetail" table="salesinvoicedetail">
        <id name="saleinvdetailid" column="saleinvdetailid">
            <generator class="increment"/>
        </id>            
        
        <property name="saleinvid" column="saleinvid"/>
        <property name="srefid" column="srefid"/>
        <property name="lotid" column="lotid"/>
        <property name="cmtval" column="cmtval"/>
        <property name="fobval" column="fobval"/>
        <property name="revenue" column="revenue"/>
        <property name="cmtuprc" column="cmtuprc"/>        
        <property name="paydate" column="paydate"/>
        <property name="qtypcs" column="qtypcs"/>
        <property name="qtyctns" column="qtyctns"/>
        <property name="ponumber" column="ponumber"/>
        <property name="etd" column="etd"/>
        <property name="remarks" column="remarks"/>
        <property name="status" column="status"/>
        <property name="delUsrId" column="delusrid"/>
        <property name="delTime" column="deltime"/>
        <property name="updUsrId" column="updusrid"/>
        <property name="updTime" column="updtime"/>
        <property name="insUsrId" column="insusrid"/>
        <property name="insTime" column="instime"/>        
    </class>
</hibernate-mapping>
